<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/white"
    tools:context=".ui.mine.activity.ReplaceActivity">
    <include
        android:id="@+id/title"
        layout="@layout/layout_title" />
     <RelativeLayout
    android:layout_below="@+id/title"
    android:background="@color/alpha_05_black"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/view0"
        android:layout_width="match_parent"
        android:layout_height="@dimen/comment_1_dip"

        android:background="@color/alpha_40_black" />
    <RelativeLayout
        android:background="@color/white"
        android:id="@+id/layout_notice"
        android:layout_width="match_parent"
        android:layout_height="@dimen/comment_50_dip"
        android:layout_below="@+id/view0">

        <TextView
            android:id="@+id/tv_notice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="@dimen/comment_18_dip"
            android:layout_centerVertical="true"
            android:text="@string/family_friend"
            android:textColor="@color/alpha_95_black"
            android:textSize="@dimen/comment_16_size" />

        <ImageView
            android:id="@+id/dynamic_selected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="@dimen/comment_11_dip"
            android:layout_centerVertical="true"
            app:srcCompat="@drawable/icon_install_selected" />
    </RelativeLayout>

    <RelativeLayout
        android:background="@color/white"
        android:layout_marginTop="@dimen/comment_10_dip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/layout_notice"
        >

        <RelativeLayout
            android:id="@+id/layout_me"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_50_dip">
            <ImageView
                android:id="@+id/img_replace"
                android:layout_width="@dimen/comment_15_dip"
                android:layout_height="@dimen/comment_15_dip"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/comment_11_dip"
                android:layout_centerVertical="true"
                app:srcCompat="@drawable/icon_full_right" />
            <TextView
                android:id="@+id/tv_me"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/comment_18_dip"
                android:layout_centerVertical="true"
                android:text="@string/not_to_see_me"
                android:textColor="@color/alpha_95_black"
                android:textSize="@dimen/comment_16_size" />
        </RelativeLayout>

        <View
            android:id="@+id/view1"
            style="@style/Mineview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_1_dip"
            android:layout_below="@+id/layout_me"
            android:background="@color/alpha_05_black" />


        <View
            android:id="@+id/view2"
            style="@style/Mineview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_1_dip"
            android:layout_below="@+id/layout_me"
            android:background="@color/alpha_05_black" />

        <RelativeLayout
            android:id="@+id/layout_her"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_50_dip"
            android:layout_below="@+id/view2">

            <TextView
                android:id="@+id/tv_her"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/comment_18_dip"
                android:text="@string/not_to_see_her"
                android:textColor="@color/alpha_95_black"
                android:textSize="@dimen/comment_16_size"
                android:layout_alignParentStart="true" />

            <ImageView
                android:id="@+id/img_new"
                android:layout_width="@dimen/comment_15_dip"
                android:layout_height="@dimen/comment_15_dip"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/comment_11_dip"
                app:srcCompat="@drawable/icon_full_right" />
        </RelativeLayout>

        <View
            android:id="@+id/view3"
            style="@style/Mineview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_1_dip"
            android:layout_below="@+id/layout_her"
            android:background="@color/alpha_05_black" />

        <RelativeLayout
            android:id="@+id/layout_three"
            android:layout_below="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_50_dip">
            <TextView
                android:id="@+id/tv_three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/comment_18_dip"
                android:text="@string/allow_friends_see"
                android:textColor="@color/alpha_95_black"
                android:textSize="@dimen/comment_16_size"
                />

            <TextView
                android:id="@+id/tv_username"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/comment_30_dip"
                android:text="@string/three_days"
                android:textColor="@color/alpha_95_black"
                android:textSize="@dimen/comment_16_size"
                />

            <ImageView
                android:id="@+id/iv_username"
                android:layout_width="@dimen/comment_15_dip"
                android:layout_height="@dimen/comment_15_dip"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/comment_11_dip"
                app:srcCompat="@drawable/icon_full_right" />
        </RelativeLayout>
        <View

            android:id="@+id/view5"
            style="@style/Mineview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_1_dip"
            android:layout_below="@+id/layout_three"
            android:background="@color/alpha_05_black" />
        <RelativeLayout
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/comment_50_dip"
            android:layout_below="@+id/view5">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/comment_18_dip"
                android:layout_centerVertical="true"
                android:text="@string/allow_Stranger_see"
                android:layout_marginBottom="@dimen/comment_5_dip"
                android:textColor="@color/alpha_95_black"
                android:textSize="@dimen/comment_16_size" />
            <ImageView
                android:id="@+id/iv_strange"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/comment_11_dip"
                android:layout_marginBottom="@dimen/comment_5_dip"
                app:srcCompat="@drawable/icon_install_unselected" />

        </RelativeLayout>
    </RelativeLayout>
     </RelativeLayout>
</RelativeLayout>
